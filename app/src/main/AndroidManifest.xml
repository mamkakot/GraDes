<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.dmitry.dinamic_creating">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.Design.Light"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".GraphListActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MatrixActivity"
            android:label="@string/matrix_title"/>
        <activity android:name=".MainActivity"
            android:label="@string/graph_list_title"/>
        <activity android:name=".AlgGlossary" />
    </application>

</manifest>